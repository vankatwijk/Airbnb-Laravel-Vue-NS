    "clean": "rimraf node_modules hooks platforms target urlhandler.*.js urlhandler.*.d.ts *.ngsummary.json *.metadata.json *.js.map  test/e2e/**/*.js test/e2e/**/*.js.map test/e2e/platforms test/e2e/node_modules test/e2e/hooks",
